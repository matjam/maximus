system os2 dll initi

debug all
name ..\dll\msgapi.dll
opt modname=msgapi
#opt symf=msgapi

opt verb,quiet
opt map=..\map\msgapi.map

path .;wcp\l

file msgapi.obl, bld.obl, api_sdm.obl

file sq_area.obl, sq_msg.obl, sq_read.obl, sq_lock.obl, sq_misc.obl
file sq_write.obl, sq_kill.obl, sq_uid.obl, sq_help.obl, sq_idx.obl

file 1stchar.obl, cvtdate.obl, date2bin.obl, dosdate.obl, fexist.obl
file ffind.obl, flush.obl, months.obl, wc_misc.obl, parsenn.obl
file qksort.obl, stristr.obl, strocpy.obl, trail.obl
file nopen.obl, setfsize.obl, tdelay.obl
#file hmalloc.obl, hread.obl#, hmemmove.obl#, hfmemmov.obl

lib clibdll, doscalls.lib

segment class 'CODE' preload executeread share

export MSGOPENAREA.1
export MSGOPENAPI.2
export MSGVALIDATE.3
export MSGCLOSEAPI.4
export CONVERTCONTROLINFO.5
export COPYTOCONTROLBUF.6
export CVTCTRLTOKLUDGE.7
export GETCTRLTOKEN.8
export NUMKLUDGES.9
export REMOVEFROMCTRL.10
export SQUISHSETMAXMSG.11
export WRITEZPINFO.12
export _msgapierr.13
export SQUISHHASH.14
export INVALIDMH.15
export INVALIDMSGH.16
export MSGFREECTRLTOKEN.17
export MSGFREECTRLBUF.18

#opt caseexact
opt nodefaultlibs
opt protmode
#opt stack=0
#opt heap=0
opt manyautodata
opt newf

