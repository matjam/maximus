# $Id: MSGAPI32.LNK 1.4 1995/05/06 17:49:55 sjd Exp $

deb all
sys os2v2 dll initi
name ..\dll\msgapi32.dll

libpath ..\..\lib;..\lib

opt verb,quiet
opt map=..\map\msgapi32.map

path .;w3p

file
{
        msgapi.obf
        bld.obf

        api_sdm.obf         sq_area.obf         sq_msg.obf
        sq_read.obf         sq_write.obf
        sq_kill.obf         sq_uid.obf          sq_lock.obf
        sq_misc.obf         sq_help.obf         sq_idx.obf
}

lib os2386

opt modname=msgapi32

export MsgOpenArea.1
export MsgOpenApi.2
export MsgValidate.3
export MsgCloseApi.4
export ConvertControlInfo.5
export CopyToControlBuf.6
export CvtCtrlToKludge.7
export GetCtrlToken.8
export NumKludges.9
export RemoveFromCtrl.10
export SquishSetMaxMsg.11
export WriteZPInfo.12
export _msgapierr.13
export SquishHash.14
export InvalidMh.15
export InvalidMsgh.16
export MsgFreeCtrlToken.17
export MsgFreeCtrlBuf.18

opt case,many
lib W3plibf
#opt protmode,stack=0,heap=0,nodef
opt newf

